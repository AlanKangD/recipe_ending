<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.care.root.recipeBoard.BoardContentMapper">
	
	<insert id="insertContent">
	insert into boardcontentDB values(2, #{id}, #{title}, #{content})
	</insert>
	
	<insert id="insertFile">
	insert into fileDB values(2, #{file1}, #{file2}, #{file3})
	</insert>
	
</mapper>