<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.care.root.KakaoLoginMapper">
	 
	<resultMap type="com.care.root.KakaoVO" id="kakao">
		<id property="id" column="id"/>
		<result property="pw" column="pw"/>
		<result property="nickname" column="nickname"/>
	</resultMap>
	

	<insert id="registerKakao" >
		insert into test(id,pw,nickname) values(#{id}, #{pw}, #{nickname})
	</insert>
 
	<select id="checkid" resultMap="kakao">
		select * from test where id=#{id} 
	</select>

	
</mapper>